FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     chemFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.001;

deltaT          1e-05;

maxDeltaT       1;

adjustTimeStep  on;

writeControl    adjustableRunTime;

writeInterval   5e-04;

purgeWrite      0;

writeFormat     ascii;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

DebugSwitches
{
    SolverPerformance   0;
}

