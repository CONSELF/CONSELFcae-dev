FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 0 0 1 0 0 0];

internalField       uniform 300;

boundaryField
{
    outlet
    {
        type               inletOutlet;
        phi                phi.water;
        inletValue         uniform 300;
        value              $internalField;
    }
    walls
    {
        type               zeroGradient;
    }
}

