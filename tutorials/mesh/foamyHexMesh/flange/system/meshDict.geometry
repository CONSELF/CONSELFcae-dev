flange.stl
{
    name flange;
    type triSurfaceMesh;

    regions
    {
        patch1
        {
            name wall;
        }

        patch2
        {
            name plate;
        }

        patch3
        {
            name rings;
        }

        patch4
        {
            name outlet;
        }
    }
