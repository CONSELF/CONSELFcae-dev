FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(D|Dcorr)"
    {
        solver          GAMG;
        tolerance       1e-06;
        relTol          0.99;
        smoother        GaussSeidel;
        nCellsInCoarsestLevel 6;
    }
}

stressAnalysis
{
    nCorrectors     1;
    D               1e-10;
    accelerationFactor 2;   // lower for other Poisson ratio (e.g. 0.1 for 0.3)
}


