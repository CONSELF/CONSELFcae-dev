T
{
    boundaryField
    {
        wallFilm
        {
            type            mapped;
            field       Tsf;
            average         300;
            setAverage      no;
            value           uniform 300;
        }
    }
}

U
{
    boundaryField
    {
        wallFilm
        {
            type            mapped;
            field       Usf;
            average         (0 0 0);
            setAverage      no;
            value           uniform (0 0 0);
        }
    }
}

"nut"
{
    boundaryField
    {
        wallFilm
        {
            value           uniform 0;
        }
    }
}

"alphat"
{
    boundaryField
